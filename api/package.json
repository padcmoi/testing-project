{
  "scripts": {
    "build": "tsup src/index.ts --format cjs --clean",
    "prod": "node ./dist/index",
    "test": "jest --coverage",
    "dev": "clear && pnpm run test && pnpm run nodemon",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "pnpm run lint --fix",
    "format": "prettier --write src/",
    "nodemon": "nodemon --watch src -e js,ts,json --exec \"ts-node src/index.ts\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.3.0",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "tsup": "^8.0.1"
  }
